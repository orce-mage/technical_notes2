function showCustomSuccessful(){jQuery('#sgcolorbox').find('.es_title_belvg').text('Thanks for subscribing!').end().find('.es_caption').html('Whenever there’s a new article on the blog,<br>you’ll receive notifications.').end().find('.js-force-close-popup').text('Enjoy Reading!').end().find('.es_shortcode_form').addClass('is-success');}
function eemail_submitresults(){if(http_req.readyState==4){if(http_req.status==200){if(http_req.readyState==4||http_req.readyState=="complete"){jQuery('.es_shortcode_form').removeClass('is-sending');document.getElementById("es_msg_pg").innerHTML='';if((http_req.responseText).trim()=="subscribed-successfully"){es_set_cookie('belvgSubscriber',true,365*10);if(isCustomPopup()){showCustomSuccessful();}else{document.getElementById("es_msg_pg").innerHTML=es_widget_page_notices.es_success_message;document.getElementById("es_txt_email_pg").value="";}}
else if((http_req.responseText).trim()=="subscribed-pending-doubleoptin"){es_set_cookie('belvgSubscriber',true,365*10);if(isCustomPopup()){showCustomSuccessful();}else{showPopup(es_widget_page_notices.es_success_notice);document.getElementById("es_msg_pg").innerHTML=es_widget_notices.es_success_message;document.getElementById("es_txt_email_pg").value="";document.getElementById("es_txt_name_pg").value="";}}
else if((http_req.responseText).trim()=="already-exist"){document.getElementById("es_msg_pg").innerHTML=es_widget_page_notices.es_email_exists;}
else if((http_req.responseText).trim()=="unexpected-error"){document.getElementById("es_msg_pg").innerHTML=es_widget_page_notices.es_error;}
else if((http_req.responseText).trim()=="invalid-email"){document.getElementById("es_msg_pg").innerHTML=es_widget_page_notices.es_invalid_email;}
else{document.getElementById("es_msg_pg").innerHTML=es_widget_page_notices.es_try_later;document.getElementById("es_txt_email_pg").value="";document.getElementById("es_txt_name_pg").value="";}}}
else{alert(es_widget_page_notices.es_problem_request);}}}
